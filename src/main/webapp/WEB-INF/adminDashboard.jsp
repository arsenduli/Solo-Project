</header>
<!-- /HEADER -->

<!-- NAVIGATION -->
<nav id="navigation">
    <!-- container -->
    <div class="container">
        <!-- responsive-nav -->
        <div id="responsive-nav">
            <!-- NAV -->
            <ul class="main-nav nav navbar-nav">
                <li class="active"><a href="/admin/dashboard">Home</a></li>
                <li><a href="/products/new">Create New Product</a></li>
                <li><a href="/category/new">Create New Category</a></li>
                <li><a href="/logout">LogOut</a></li>
            </ul>
            <!-- /NAV -->
        </div>
        <!-- /responsive-nav -->
    </div>
    <!-- /container -->
</nav>
<!-- /NAVIGATION -->

<div class="section">
    <!-- container -->
    <div class="container">
        <!-- row -->
        <div class="row">

<h1>Add Products To Category</h1>
        <c:forEach var="category" items="${categories}">
            <div class="col-md-4 col-xs-6">
                <div class="shop">
                    <div class="shop-img">
                        <img src="${category.urlCategori}" alt="">
                    </div>
                    <div class="shop-body">
                        <h3>${category.name}<br>Collection</h3>
                        <a href="/admin/categories/${category.id}" class="cta-btn">Add Product <i class="fa fa-arrow-circle-right"></i></a>
                    </div>
                </div>
            </div>
        </c:forEach>

        </div>
        <!-- /row -->
    </div>
    <!-- /container -->
</div>
<!-- /SECTION -->

<!-- SECTION -->
<div class="section">
    <!-- container -->
    <div class="container">
        <!-- row -->
        <div class="row">

            <!-- section title -->
            <div class="col-md-12">
                <div class="section-title">
                    <h3 class="title">My Product</h3>
                </div>
            </div>
            <!-- /section title -->

            <!-- Products tab & slick -->
            <div class="col-md-12">
                <div class="row">
                    <div class="products-tabs">
                        <!-- tab -->
                        <div id="tab1" class="tab-pane active">
                            <div class="products-slick" data-nav="#slick-nav-1">
                                <!-- product -->
                                <c:forEach items="${proAdmn}" var="productsVar">
                                    <div class="product">
                                        <div class="product-img">
                                            <img src="${productsVar.url}" alt="not good"/>
                                            <div class="product-label">
                                            </div>
                                        </div>
                                        <div class="product-body">
                                            <p class="product-category"></p>
                                            <h3 class="product-name"><a >${productsVar.name}</a></h3>
                                            <h4 class="product-price">$ ${productsVar.price}</h4>
                                            <div class="product-rating">
                                                <i class="fa fa-star"></i>
                                                <i class="fa fa-star"></i>
                                                <i class="fa fa-star"></i>
                                                <i class="fa fa-star"></i>
                                                <i class="fa fa-star"></i>
                                            </div>
                                                <div class="product-btns">
                                                    <a href="/products/${productsVar.id}/delete" class="quick-view"><span class="tooltipp">Delete ||</span></a>
                                                    <a href="/products/edit/${productsVar.id}" class="quick-view"><span class="tooltipp">Edit</span></a>
                                                </div>
                                        </div>

                                    </div>
                                </c:forEach>
                                <!-- /product -->

                            </div>
                            <div id="slick-nav-1" class="products-slick-nav"></div>
                        </div>
                        <!-- /tab -->
                    </div>
                </div>
            </div>
            <!-- Products tab & slick -->
        </div>
        <!-- /row -->
    </div>
    <!-- /container -->
</div>

